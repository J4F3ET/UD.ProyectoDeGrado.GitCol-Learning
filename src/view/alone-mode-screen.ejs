<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta
			name="viewport"
			content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"
		/>
		<link
			async
			rel="shortcut icon"
			href="/dist/assets/fav-icon.ico"
			type="image/x-icon"
		/>
		<title>Git Col-Learning</title>
		<!-- Style global -->
		<link rel="stylesheet" href="/dist/css/reset.css" />
		<link defer rel="stylesheet" href="/dist/css/global.css" />
		<link defer rel="stylesheet" href="/dist/css/font-global.css" />
		<link rel="stylesheet" href="/dist/css/icons-global.css" />
		<link async rel="stylesheet" href="/dist/css/cursor-global.css" />
		<!-- Style screen -->
		<link async rel="stylesheet" href="/dist/css/work-style.css" />
		<link async rel="stylesheet" href="/dist/css/dialog-home-style.css" />

		<link async rel="stylesheet" href="/dist/css/dataViewer/elementSVG.css" />
		<link async rel="stylesheet" href="/dist/css/dataViewer/circle.css" />
		<link async rel="stylesheet" href="/dist/css/dataViewer/line.css" />
		<link async rel="stylesheet" href="/dist/css/dataViewer/tag.css" />
		<link async rel="stylesheet" href="/dist/css/dataViewer/text.css" />
		<link async rel="stylesheet" href="/dist/css/dataViewer/log.css" />
		<link async rel="stylesheet" href="/dist/css/driverjs-style.css" />

		<link async rel="stylesheet" href="/dist/css/input-concept.css" />

		<!-- Drivejs -->
		<link
			async
			rel="stylesheet"
			href="https://cdn.jsdelivr.net/npm/driver.js@latest/dist/driver.css"
		/>
		<script
			async
			src="https://cdn.jsdelivr.net/npm/driver.js@latest/dist/driver.js.iife.js"
		></script>
		<script async src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
		<script async>
			const REF_STORAGE_REPOSITORY = "local";
			const REF_STORAGE_REPOSITORY_CLOUD = null;
			const REF_STORAGE_LOG = "log";
			const CONCEPT = <%- JSON.stringify(concept) %>;
		</script>

		<!-- Script -->
		<script defer type="module" src="/dist/js/mode-script.js"></script>
		<script defer type="module" src="/dist/js/drivejs-mode-script.js"></script>
		<script defer type="module" src="/dist/js/utils/concept-config.js"></script>

		<script async type="module" src="/dist/js/alone-mode-script.js"></script>
		<script
			defer
			type="module"
			src="/dist/js/utils/btn-session-handler.js"
		></script>
		<script async type="module" src="/dist/js/utils/handler-nolog.js"></script>
		<script async src="/dist/js/dialogs/dialog-login-home-script.js"></script>
		<script defer type="module" src="/dist/js/login-script.js"></script>
		<script
			async
			type="module"
			src="/dist/js/dialogs/dialog-question-concept-script.js"
		></script>
		<script async src="/dist/js/banner.js"></script>
	</head>
	<body class="scrollable body">
		<section class="container-box-control" id="controlBoxContainer">
			<div id="logContainer" class="container-log shell scrollable"></div>
			<div class="container-input-shell shell" id="inputContainer">
				$
				<input
					id="comandInput"
					type="text"
					placeholder="comand"
					class="input"
				/>
			</div>
		</section>
		<section class="container-help-and-svg" id="contentContainer">
			<div class="container-panel-help" id="panelHelpContainer">
				<select id="select_concept" class="input_concept">
					<option value="free-mode" <%= current === "free-mode" ? "selected" : "" %>>
						Free Mode
					  </option>
					  

					<% challenges.forEach((challenge) => { %>
					  <option value="<%= challenge %>"
						<% if (challenge == current) { %>
						  selected
						<% } %>
					>
						<%= challenge
						  .replace(/-/g, " ")
						  .replace(/\b\w/g, l => l.toUpperCase()) %>
					  </option>
					<% }) %>
				  </select>
				<button
					id="btnTutorial"
					class="btn orange btn-icon-string"
					title="solution"
				>
					<i class="icon-exclamation-circle"> </i>&nbsp;&nbsp;Tutorial
				</button>
				<button id="btn_login" class="btn green" title="solution">Login</button>
				<button
					disabled
					style="display: none"
					id="btnLogout"
					class="btn red"
					title="back"
				>
					Logout
				</button>

				<button id="btnExit" class="btn red btn-only-icon" title="back">
					<i class="icon-times"></i>
				</button>
			</div>
			<div id="svgContainer" class="container-svg scrollable"></div>
		</section>
		<!-- DIALOG LOGIN -->
		<dialog id="dialog_login" data-dialog="login" class="dialog second">
			<section class="view-section container_dialog">
				<div class="btn_dialog_close">
					<button
						id="btn_close_login"
						data-dialog="dialog_login"
						class="close_dialog btn-times btn red"
					>
						<i data-dialog="dialog_multi" class="icon-times white"></i>
					</button>
				</div>
				<div class="section_dialog info">
					<div class="section_dialog_item">
						<h2 class="title_dialog">Login:</h2>
						<p class="text_dialog">Select a supplier to log in</p>
					</div>
					<div class="section_dialog_item group_buttons_dialog">
						<button
							id="googleLogin"
							data-dialog="dialog_multi"
							class="item_dialog close_dialog btn orange"
						>
							<div
								data-dialog="dialog_multi"
								class="icon-provider-container-dialog"
							>
								<p data-dialog="dialog_multi">Gmail</p>
							</div>
						</button>
						<button
							id="microsoftLogin"
							data-dialog="dialog_multi"
							class="item_dialog close_dialog btn orange"
						>
							<div
								data-dialog="dialog_multi"
								class="icon-provider-container-dialog"
							>
								<p data-dialog="dialog_multi">Microsoft</p>
							</div>
						</button>
						<button
							id="githubLogin"
							data-dialog="dialog_multi"
							class="item_dialog close_dialog btn orange"
						>
							<div
								data-dialog="dialog_multi"
								class="icon-provider-container-dialog"
							>
								<p data-dialog="dialog_multi">GitHub</p>
							</div>
						</button>
					</div>
				</div>
				<div class="section_dialog img">
					<div class="section_dialog_item img dialog_block_img">
						<img
							defer
							loading="lazy"
							class="img_cat multi"
							src="/dist/assets/img/home/cats.webp"
							alt="Multi user mode"
						/>
					</div>
				</div>
			</section>
		</dialog>
		<!-- END DIALOG LOGIN -->
		<!-- DIALOG QUESTION -->
		<dialog id="dialog_question" class="dialog second">
			<section class="view-section container_dialog">
				<div class="btn_dialog_close">
					<button
						id="btn_close_question"
						data-dialog="dialog_question"
						class="btn_question close_dialog btn-times btn red"
					>
						<i data-dialog="dialog_question" class="icon-times white"></i>
					</button>
				</div>
				<div class="section_dialog info">
					<div class="section_dialog_item">
						<h2 class="title_dialog">Question</h2>
						<p class="text_dialog">The question is the following:</p>
					</div>
					<div class="section_dialog_item">
						<p class="text_dialog">
							<span class="question-text"><%= concept?.question?.text %></span>
						</p>
					</div>
					<div class="section_dialog_item">
						<% concept?.question?.options.forEach(function(option) { %>
						<div class="option-container">
							<input type="radio" name="question-options" id="<%= option %>" />
							<label for="<%= option %>">
								<span class="option-text"><%= option %></span>
							</label>
						</div>
						<% }) %>
					</div>
					<div class="section_dialog_item group_buttons_dialog">
						<button
							id="btn_answer_question"
							class="btn_question item_dialog close_dialog btn orange"
						>
							<span>Answer</span>
						</button>
						<button
							id="btn_cancel_question"
							class="btn_question item_dialog close_dialog btn red"
						>
							<span>Cancel</span>
						</button>
					</div>
				</div>
			</section>
		</dialog>
	</body>
</html>
